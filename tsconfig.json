{
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext"],
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noErrorTruncation": false,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
  ],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
